# editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = false

[*.html]
[*.js]
[*.json]
[*.liquidrc]
[*.config.js]
insert_final_newline = false

[*.liquid]
indent_size = 4
indent_style = tab
insert_final_newline = false
